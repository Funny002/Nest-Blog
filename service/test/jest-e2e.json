{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/mysql/(.*)": "<rootDir>/../libs/mysql/src/$1",
    "@app/mysql": "<rootDir>/../libs/mysql/src",
    "@app/swagger/(.*)": "<rootDir>/../libs/swagger/src/$1",
    "@app/swagger": "<rootDir>/../libs/swagger/src"
  }
}